#include "stdafx.h"
#include <iostream>
#include "conio.h"


using namespace std;

void main()

{
	int numeros [100],n, menor= 9999999;
	
   
	
	cout<<" digite el numerode elementos del arreglo del vector "<<endl;
	cin>>n;

	
	for(int i=0;i<n;i++){
		cout<<i+1<<"ingrese los numeros "<<endl;
		cin>>numeros[i];



		if(numeros [i]<menor)
		{menor = numeros[i];
	
		}

	}
	

	cout<<"El menor de los numeros es : "<<menor<<endl;

	getch();

}
