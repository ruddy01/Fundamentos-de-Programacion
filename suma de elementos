#include "stdafx.h"
#include <iostream>
#include "conio.h"


using namespace std;

void main()

{
	int n ,numeros [5];
	int suma =0;
   
	
	cout<<" ingrese numeros de elementos del vector "<<endl;
	cin>>n;

	
	for(int i=0;i<n;i++){

	cout<<"Ingrese el elemento : ["<<i<<"]:";
	cin>> numeros[i];

	suma+= numeros [i];
	
	}

	cout<<"lA SUMA DE LOS ELEMENTOS ES : "<<suma<<endl;

	getch();

}
