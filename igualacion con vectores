#include "stdafx.h"
#include "iostream"
#include "conio.h"

using namespace std;
void main()

{
	int salario[100],n;
	char empleadocod[10], emple;
	int mayor=0;
	


	cout<<"digite el numero de empleados :";
	cin>> n;

	   for(int i=0; i<n;i++)
	     {
		   cout<<i+1<< ".Digite el codigo del empleado:";
	        cin >> empleadocod[i];
			cout<<i+1<< ".Digite el salaerio del empleado:";
	        cin >> salario[i];


			if (salario[i]>mayor)
			{mayor=salario[i];
			emple=empleadocod[i];

	   } 
	   }
			
		   
		   cout<<"el mayorsalario es:.... :"<<mayor<<endl;
		   cout<<" el codigo de ese empleado es :"<<emple<<endl;
	     		 
				 
getch();
}
